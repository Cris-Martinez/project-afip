	<queryString>
		<![CDATA[select vta.numero, vta.fecha, vta.cliente, vta.domicilio, vta.cuit, vta.neto as tNeto,
vta.iva as tIva, vta.total as tTotal, vta.categoriaiva, pro.codigo_interno, pro.nombre, pro.codigo, dv.bulto, dv.pieza, dv.cantidad, dv.precio, dv.neto, dv.iva,
(dv.precio * dv.cantidad) as linea_detalle_total,
((dv.bulto * pro.bulto + dv.pieza) * pro.pieza) + dv.cantidad as lieas_cantidad_total
 from vista_ventas as vta
 JOIN v_detalle_ventas as dv on dv.venta_id::int = vta.id::int
 JOIN v_productos as pro on pro.id::int = dv.producto_id::int
 where vta.id::int = $P{entidad_id}]]>
	</queryString>
